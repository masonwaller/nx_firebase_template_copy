{
  "name": "nx-template",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "nx run-many --all --target=build --skip-nx-cache",
    "build:prod": "nx run-many --all --target=build --skip-nx-cache --configuration=production",
    "dev": "nx run-many --all --target=serve --skip-nx-cache",
    "dev:prod": "nx run-many --all --target=serve --skip-nx-cache --configuration=production",
    "start": "nx run-many --all --target=serve --skip-nx-cache",
    "test": "nx run-many --all --target=test --skip-nx-cache",
    "lint": "npx nx run-many --all --target=lint --skip-nx-cache",
    "test:lint": "npm run lint && npm run test",
    "test:affected": "nx affected:test --codeCoverage",
    "test:codeCoverage": "nx run-many --all --target=test --skip-nx-cache --codeCoverage",
    "deploy:firebase:api:dev": "nx run server:build-firebase --skip-nx-cache && firebase use nx-firebase-template-test && firebase deploy --only functions"
  },
  "private": true,
  "dependencies": {
    "@firebase/firestore": "^3.12.2",
    "@firebase/storage": "^0.11.2",
    "@headlessui/react": "^1.7.15",
    "@nestjs/common": "^9.1.1",
    "@nestjs/core": "^9.1.1",
    "@nestjs/platform-express": "^9.4.3",
    "@nx/nest": "^16.1.4",
    "@react-oauth/google": "^0.11.0",
    "@sendgrid/mail": "^7.7.0",
    "@swc/helpers": "~0.5.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "axios": "^1.0.0",
    "clsx": "^1.2.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "firebase-admin": "^11.8.0",
    "firebase-functions": "^4.4.0",
    "prism": "^4.1.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "^6.11.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.14.5",
    "@nestjs/schematics": "^9.1.0",
    "@nestjs/testing": "^9.1.1",
    "@nx/cypress": "16.1.4",
    "@nx/eslint-plugin": "16.1.4",
    "@nx/jest": "16.1.4",
    "@nx/js": "16.1.4",
    "@nx/linter": "16.1.4",
    "@nx/node": "16.1.4",
    "@nx/react": "16.1.4",
    "@nx/webpack": "16.1.4",
    "@nx/workspace": "16.1.4",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@svgr/webpack": "^6.1.2",
    "@swc/cli": "~0.1.62",
    "@swc/core": "~1.3.51",
    "@testing-library/react": "14.0.0",
    "@types/jest": "^29.4.0",
    "@types/node": "18.14.2",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "autoprefixer": "^10.4.14",
    "babel-jest": "^29.4.1",
    "cypress": "^12.11.0",
